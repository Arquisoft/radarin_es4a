{
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@inrupt/solid-react-components": "0.5.3",
    "@inrupt/solid-style-guide": "^0.1.34",
    "@material-ui/core": "^4.11.4",
    "@material-ui/data-grid": "^4.0.0-alpha.27",
    "@solid/query-ldflex": "^2.11.3",
    "axios": "^0.21.1",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "express": "^4.17.1",
    "flag-icon-css": "^3.3.0",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "google-map-react": "^2.1.9",
    "i18next": "^15.0.4",
    "i18next-browser-languagedetector": "^3.0.1",
    "i18next-xhr-backend": "^2.0.1",
    "normalize.css": "^8.0.1",
    "npm-watch": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-bootstrap": "^1.5.2",
    "react-click-outside": "^3.0.1",
    "react-dev-utils": "^7.0.1",
    "react-dom": "^17.0.2",
    "react-i18next": "^10.1.2",
    "react-modal": "^3.13.1",
    "react-test-renderer": "^17.0.2",
    "react-toastify": "^5.5.0",
    "styled-components": "^4.1.3",
    "use-position": "0.0.8"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --coverage",
    "test:e2e": "cd e2e && jest",
    "docs": "shx rm -rf build/docs && asciidoctor -D build/docs -a imagesdir=./images -r asciidoctor-diagram docs/index.adoc && shx cp -R docs/images build/docs",
    "eject": "react-scripts eject",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "webapp"
      ],
      "extensions": "js,jsx"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*(*.)@(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@components": "./src/components",
            "@containers": "./src/containers",
            "@util-components": "./src/components/Utils",
            "@services": "./src/services",
            "@layouts": "./src/layouts",
            "@contexts": "./src/contexts/",
            "@hocs": "./src/hocs",
            "@utils": "./src/utils",
            "@hooks": "./src/hooks",
            "@constants": "./src/constants",
            "@vocabs": "./src/vocabs"
          }
        }
      ]
    ]
  },
  "name": "solid-app",
  "version": "0.1.0",
  "private": false,
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/runtime": "^7.14.0",
    "@testing-library/react": "^11.2.6",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "8.0.4",
    "babel-plugin-module-resolver": "^3.1.2",
    "babel-plugin-named-asset-import": "^0.3.0",
    "babel-preset-react-app": "^7.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "1.0.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-config-xo": "^0.25.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "^5.7.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "expect-puppeteer": "^4.4.0",
    "file-loader": "2.0.0",
    "html-webpack-include-assets-plugin": "^1.0.6",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "husky": "^1.3.1",
    "jest": "^25.5.4",
    "jest-cucumber": "^3.0.0",
    "jest-dev-server": "^4.4.0",
    "jest-dom": "^3.1.2",
    "jest-environment-node": "^26.6.2",
    "jest-pnp-resolver": "1.0.1",
    "jest-puppeteer": "^4.4.0",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.3.1",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.18.2",
    "puppeteer": "^5.5.0",
    "react-testing-library": "^6.0.0",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "shx": "^0.3.3",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "^2.3.5",
    "url-loader": "1.1.1",
    "webpack": "^4.46.0",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  }
}
